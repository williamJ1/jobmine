<!--<p align="right">This is header</p>-->
<% if @user = User.find_by(id: session[:current_user_id])%>
    <p align="right">Hello <%= @email = @user.email%> !</p>
    <ul class="nav justify-content-center">
      <li class="nav-item">
        <%= link_to "Log out", logout_path, method: :get, class:"nav-link active" %></br>
      </li>
      <li class="nav-item">
        <%= link_to "my home page", jobs_path, method: :get, class:"nav-link active" %></br>
      </li>
      <li class="nav-item">
        <%= link_to "my profile", show_profile_path(id: session[:current_user_id]), class:"nav-link active"  %></br>
    </ul>



<% else%>
    <% if flash[:info] %>
        <p align="right">You have successfully logged out!</p></br>
    <% else%>
     <!--   <p align="right">You are not logged in yet!</p></br>-->

    <%end%>
<% end%>
